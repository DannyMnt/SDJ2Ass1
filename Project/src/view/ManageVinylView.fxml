<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="361.0" prefWidth="530.0" userData="Manage Exercise" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ManageVinylViewController">
   <children>
      <HBox alignment="CENTER" layoutX="20.0" layoutY="79.0" spacing="10.0">
         <children>
            <Label fx:id="headerLabel" alignment="CENTER" prefHeight="35.0" prefWidth="519.0" text="Manage Vinyl">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="514.0" spacing="10.0">
         <children>
            <Label text="Title">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="titleLabel" onAction="#onEnter" prefHeight="39.0" prefWidth="105.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Label layoutX="10.0" layoutY="16.0" text="Artist">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="artistLabel" layoutX="80.0" layoutY="10.0" onAction="#onEnter" prefHeight="39.0" prefWidth="61.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
            <Label text="Year">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="yearLabel" onAction="#onEnter" prefHeight="39.0" prefWidth="82.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="20.0" prefHeight="39.0" prefWidth="492.0" spacing="10.0">
         <children>
            <Label prefHeight="23.0" prefWidth="70.0" text="Name">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="nameLabel" onAction="#onEnter" prefHeight="39.0" prefWidth="252.0">
               <font>
                  <Font size="18.0" />
               </font>
            </TextField>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <VBox alignment="CENTER" prefHeight="89.0" prefWidth="421.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="reserveLabel" prefHeight="27.0" prefWidth="240.0" text="Reserved by" textAlignment="CENTER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="reserveName" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label fx:id="borrowLabel" prefHeight="27.0" prefWidth="241.0" text="Borrowed by" textAlignment="CENTER">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
                  <Label fx:id="borrowName" />
               </children>
            </HBox>
         </children>
         <opaqueInsets>
            <Insets left="5.0" right="5.0" />
         </opaqueInsets>
      </VBox>
      <HBox alignment="CENTER_LEFT" layoutX="20.0" layoutY="79.0" prefHeight="27.0" prefWidth="574.0" spacing="10.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
         <children>
            <Label fx:id="errorLabel" prefHeight="27.0" prefWidth="506.0" text="Error" textFill="RED">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutX="20.0" layoutY="79.0" spacing="30.0">
         <children>
            <Button fx:id="reserveButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#onReserve" text="Reserve">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="borrowButton" mnemonicParsing="false" onAction="#onBorrow" text="Borrow">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="returnButton" mnemonicParsing="false" onAction="#onReturn" text="Return">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <Button fx:id="removeButton" mnemonicParsing="false" onAction="#onRemove" text="Remove">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" spacing="30.0">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancel" text="Cancel">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
